@model META.DOMAIN.Emissoras

@{
    ViewData["Title"] = "EditarEmissora";
}

<h1>EditarEmissora</h1>

<h4>Emissoras</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="SalvarEdicao">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="ID" class="control-label"></label>
                <input asp-for="ID" class="form-control" />
                <span asp-validation-for="ID" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="EMISSORAS" class="control-label"></label>
                <input asp-for="EMISSORAS" class="form-control" />
                <span asp-validation-for="EMISSORAS" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Voltar</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
